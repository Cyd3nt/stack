  "Deployment & Servers": {
    "Ollama": { "type": "winget", "command": "winget install -e --id Ollama.Ollama" },
    "LM Studio": { "type": "winget", "command": "winget install -e --id Techulus.LMStudio" },
    "FastAPI": { "type": "pip", "command": "pip install fastapi" },
    "Uvicorn": { "type": "pip", "command": "pip install uvicorn" },
    "Hugging Face CLI": { "type": "pip", "command": "pip install -U huggingface_hub" }
  },
  "Advanced Tools": {
    "CUDA Toolkit": { "type": "winget", "command": "winget install -e --id NVIDIA.CUDA" },
