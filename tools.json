{
  "Essentials": {
    "Python 3.11": { "type": "winget", "command": "winget install -e --id Python.Python.3.11" },
    "Git": { "type": "winget", "command": "winget install -e --id Git.Git" },
    "Docker Desktop": { "type": "winget", "command": "winget install -e --id Docker.DockerDesktop" },
    "PowerShell 7": { "type": "winget", "command": "winget install -e --id Microsoft.PowerShell" },
    "Windows Terminal": { "type": "winget", "command": "winget install -e --id Microsoft.WindowsTerminal" }
  },
  "Core ML/DL Frameworks": {
    "PyTorch": { "type": "pip", "command": "pip install torch torchvision torchaudio" },
    "TensorFlow": { "type": "pip", "command": "pip install tensorflow" },
    "ONNX": { "type": "pip", "command": "pip install onnx" },
    "Transformers": { "type": "pip", "command": "pip install transformers" }
  },
  "Notebooks & IDEs": {
    "VS Code": { "type": "winget", "command": "winget install -e --id Microsoft.VisualStudioCode" },
    "Jupyter Lab": { "type": "pip", "command": "pip install jupyterlab" },
